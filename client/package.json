{
  "name": "borrow-buddy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --modern",
    "lint": "vue-cli-service lint && npm run wotan:fix",
    "tslint:fix": "tslint -p . -c tslint.json --fix",
    "wotan:fix": "wotan -p . -c tslint.json --fix"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "register-service-worker": "^1.0.0",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@fimbul/valtyr": "^0.10.0",
    "@fimbul/ve": "^0.10.0",
    "@fimbul/wotan": "^0.10.0",
    "@vue/cli-plugin-babel": "^3.0.0-beta.15",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.15",
    "@vue/cli-plugin-pwa": "^3.0.0-beta.15",
    "@vue/cli-plugin-typescript": "^3.0.0-beta.15",
    "@vue/cli-service": "^3.0.0-beta.15",
    "@vue/eslint-config-prettier": "^3.0.0-beta.15",
    "@vue/eslint-config-typescript": "^3.0.0-beta.15",
    "lint-staged": "^7.0.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "tslint": "^5.10.0",
    "vue-template-compiler": "^2.5.16"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add",
      "vue-cli-service lint",
      "git add"
    ],
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
