{
  "name": "borrow-buddy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --modern",
    "lint": "vue-cli-service lint && npm run wotan:fix",
    "tslint:fix": "tslint -p . -c tslint.json --fix",
    "wotan:fix": "wotan -p . -c tslint.json --fix"
  },
  "dependencies": {
    "applicationinsights-js": "^1.0.20",
    "axios": "^0.18.0",
    "register-service-worker": "^1.5.2",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuetify": "^1.3.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@fimbul/valtyr": "^0.15.0",
    "@fimbul/ve": "^0.15.0",
    "@fimbul/wotan": "^0.15.0",
    "@types/applicationinsights-js": "^1.0.7",
    "@types/node": "^10.12.0",
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-plugin-pwa": "^3.0.5",
    "@vue/cli-plugin-typescript": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "@vue/eslint-config-prettier": "^3.0.5",
    "@vue/eslint-config-typescript": "^3.0.5",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "node-sass": "^4.9.4",
    "sass-loader": "^7.1.0",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3",
    "vue-template-compiler": "^2.5.17"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add",
      "vue-cli-service lint",
      "git add"
    ],
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
